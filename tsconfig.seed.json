{
  "$schema": "https://json.schemastore.org/tsconfig",

  "compilerOptions": {
    "lib": ["ES2021", "dom"],
    "module": "commonjs",
    "target": "ES2021",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "outDir": "seeds"
  },
  "include": ["prisma/**/*"]
}
